---
spring:
    profiles: primary
server:
    port: 8011

eureka:
  instance:
    hostname: localhost
  client:
    registerWithEureka: true
    fetchRegistry: true
    serviceUrl:
      defaultZone: http://localhost:8012/eureka/,http://localhost:8013/eureka/
---
